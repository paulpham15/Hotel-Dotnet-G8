@using Microsoft.AspNetCore.Identity
@using System.Security.Claims
@inject SignInManager<HotelDotNet.Data.User> SignInManager
@inject UserManager<HotelDotNet.Data.User> UserManager
<!DOCTYPE html>
<html dir="ltr" lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <meta name="keywords"
          content="hotel booking, .net, asp dot net, booking">
    <meta name="description"
          content="Websit to booking hotel in Viet Nam, develop by G8">
    <meta name="robots" content="noindex,nofollow">
    <title>@ViewData["Title"] - HotelDotNet</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="//cdn.jsdelivr.net/chartist.js/latest/chartist.min.css">
    <link rel="stylesheet" href="~/HotelDotNet.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="//use.fontawesome.com/releases/v5.0.7/css/all.css">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,500;1,100;1,300;1,500;1,700&display=swap" rel="stylesheet">

    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!-- Tell the browser to be responsive to screen width -->

    <title>Websit to booking hotel in Viet Nam, develop by G8</title>
    <link rel="canonical" href="https://www.wrappixel.com/templates/xtreme-admin-lite/" />

    <!-- Custom CSS -->
    <link href="~/lib/chartist/dist/chartist.min.css" rel="stylesheet">
    <!-- Custom CSS -->

    <link href="~/css/style.min.css" rel="stylesheet">
    <link rel="stylesheet" href="https://unpkg.com/multiple-select@1.5.2/dist/multiple-select.min.css">
    <link href="~/css/materialdesignicons.min.css" media="all" rel="stylesheet" type="text/css" />
    <link href="https://github.com/davidstutz/bootstrap-multiselect/blob/master/dist/css/bootstrap-multiselect.min.css" rel="stylesheet" type="text/css" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-multiselect/1.1.2/css/bootstrap-multiselect.css" rel="stylesheet" type="text/css" />
</head>

<body>
    <!-- ============================================================== -->
    <!-- Preloader - style you can find in spinners.css -->
    <!-- ============================================================== -->
    <div class="preloader">
        <div class="lds-ripple">
            <div class="lds-pos"></div>
            <div class="lds-pos"></div>
        </div>
    </div>
    <!-- ============================================================== -->
    <!-- Main wrapper - style you can find in pages.scss -->
    <!-- ============================================================== -->
    <div id="main-wrapper" data-layout="vertical" data-navbarbg="skin5" data-sidebartype="full"
         data-sidebar-position="absolute" data-header-position="absolute" data-boxed-layout="full">
        <!-- ============================================================== -->
        <!-- Topbar header - style you can find in pages.scss -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- End Topbar header -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <aside class="left-sidebar border-radius-sidebar" data-sidebarbg="skin6">
            <!-- Sidebar scroll-->
            <div class="scroll-sidebar">
                <!-- Sidebar navigation-->
                <nav class="sidebar-nav">
                    <ul id="sidebarnav">
                        <!-- User Profile-->
                        <!-- User Profile-->
                        <li class="brand-container">
                            <a href="#">
                                <span class="brand-title hide-menu m-l-5"><span style="color: black;">G8</span>Booking</span>
                            </a>

                        </li>
                        </br>
                        <!-- End User Profile-->
                        @*<li class="p-15 m-t-10">
                            <a href="javascript:void(0)"
                               class="btn d-block w-100 create-btn text-white no-block d-flex align-items-center">
                                <i class="fa fa-plus-square"></i> <span class="hide-menu m-l-5">Create New</span>
                            </a>
                        </li>*@
                        <!-- User Profile-->
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link"
                               aria-expanded="false" asp-area="Admin" asp-controller="Dashboard" asp-action="Index">
                                <i class="mdi mdi-view-dashboard"></i><span class="hide-menu menu-title">Dashboard</span>
                            </a>
                        </li>
                        </br>
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link"
                               aria-expanded="false" asp-area="Admin" asp-controller="Hotel" asp-action="Index">
                                <i class="mdi mdi-folder-plus"></i><span class="hide-menu menu-title">Hotel</span>
                            </a>
                        </li>
                        </br>
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link"
                               aria-expanded="false" asp-area="Admin" asp-controller="User" asp-action="Index">
                                <i class="mdi mdi-account"></i><span class="hide-menu menu-title">User</span>
                            </a>
                        </li>
                        </br>
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link"
                               aria-expanded="false" asp-area="Admin" asp-controller="Booking" asp-action="Index">
                                <i class="mdi mdi-book-open"></i><span class="hide-menu menu-title">Booking</span>
                            </a>
                        </li>
                        </br>
                        <li class="sidebar-item">
                            <a class="sidebar-link waves-effect waves-dark sidebar-link"
                               aria-expanded="false" asp-area="Admin" asp-controller="Report" asp-action="Index">
                                <i class="mdi mdi-chart-bar"></i><span class="hide-menu menu-title">Report</span>
                            </a>
                        </li>


                    </ul>
                    <!-- End Menu bar -->
                    <!-- Start User -->
                    @if (SignInManager.IsSignedIn(User))
                    {<div class="user-profile d-flex no-block dropdown m-t-20">
                            <div class="user-pic">
                                <img src="data:image/jpeg;base64," alt="users"
                                     class="rounded-circle" width="40" />
                            </div>
                            <div class="user-content hide-menu m-l-10">
                                <a href="#" class="" id="Userdd" role="button"
                                   data-bs-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                    <h5 class="m-b-0 user-name font-medium">
                                        @User.Identity.Name <i class="fa fa-angle-down"></i>
                                    </h5>
                                    <span class="op-5 user-email">@User.FindFirstValue(ClaimTypes.Role)</span>
                                </a>
                                <div class="dropdown-menu dropdown-menu-end" aria-labelledby="Userdd">

                                    <a class="dropdown-item" href="javascript:void(0)">

                                        <form class="form-inline" asp-area="Identity" asp-page="/Account/Logout" asp-route-returnUrl="@Url.Action("Index", "HomePage", new { area = "" })">
                                            <button type="submit" class="nav-link btn btn-link text-dark">Logout <i class="mdi mdi-logout"></i></button>
                                        </form>
                                    </a>
                                </div>
                            </div>
                        </div>}
                    else
                    {
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="Identity" asp-page="/Account/Register">Register</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="Identity" asp-page="/Account/Login">Login</a>
                        </li>
                    }

                    <!-- End User -->
                </nav>
                <!-- End Sidebar navigation -->
            </div>
            <!-- End Sidebar scroll-->
        </aside>
        <!-- ============================================================== -->
        <!-- End Left Sidebar - style you can find in sidebar.scss  -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- Page wrapper  -->
        <!-- ============================================================== -->
        <div class="page-wrapper">
            <div class="card container-fluid">
                <div class="row card-body">
                    @RenderBody()
                </div>
            </div>

        </div>
        <!-- ============================================================== -->
        <!-- End Wrapper -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- All Jquery -->
        <!-- ============================================================== -->
        <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.7.2/Chart.bundle.min.js"></script>
        <script src="https://code.jquery.com/jquery-3.3.1.min.js"></script>
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="//cdn.jsdelivr.net/chartist.js/latest/chartist.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="https://cdn.datatables.net/1.11.3/js/jquery.dataTables.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        <script src="https://cdn.datatables.net/1.11.3/js/dataTables.bootstrap5.min.js"></script>
        <script src="https://code.jquery.com/ui/1.13.0/jquery-ui.js"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-multiselect/1.1.2/js/bootstrap-multiselect.min.js" integrity="sha512-lxQ4VnKKW7foGFV6L9zlSe+6QppP9B2t+tMMaV4s4iqAv4iHIyXED7O+fke1VeLNaRdoVkVt8Hw/jmZ+XocsXQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
        <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-multiselect/1.1.2/js/bootstrap-multiselect.js" integrity="sha512-YwbKCcfMdqB6NYfdzp1NtNcopsG84SxP8Wxk0FgUyTvgtQe0tQRRnnFOwK3xfnZ2XYls+rCfBrD0L2EqmSD2sA==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>
        <script src="https://unpkg.com/multiple-select@1.5.2/dist/multiple-select.min.js"></script>


        <!--Wave Effects -->
        <script src="~/js/waves.js"></script>
        <!--Menu sidebar -->
        <script src="~/js/sidebarmenu.js"></script>
        <!--Custom JavaScript -->
        <script src="~/lib/chartist/dist/chartist.min.js"></script>
        <script src="~/lib/chartist-plugin-tooltips/dist/chartist-plugin-tooltip.min.js"></script>
        <script src="~/js/custom.js"></script>
        <script src="~/js/app-style-switcher.js"></script>

        <!--This page JavaScript -->
        <!--chartis chart-->
        <script src="~/js/dashboard1.js"></script>
        <script>$(function () {
                $.extend(true, $.fn.dataTable.defaults, {
                    language: {
                        search: ""
                    }
                });

                $('.table').DataTable();
                $('[type=search]').each(function () {
                    $(this).attr("placeholder", "Search...");
                    $(this).before('<span class="fa fa-search"></span>');
                });
                $(".datepicker").datepicker({
                    dateFormat: "yy-mm-dd"
                });
                $('#example-getting-started').multiselect();
                $('#select-box').multipleSelect(
                    { showClear: true }
                );


            });</script>
        @await RenderSectionAsync("Scripts", required: false)
</body>

</html>